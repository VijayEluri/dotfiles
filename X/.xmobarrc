Config {
  font = "xft:DejaVu Sans Mono-12",
  bgColor = "black",
  fgColor = "lightgreen",
  position = BottomW L 90,
  lowerOnStart = True,
  commands = [ Run Weather "KMDW" ["-t", "<tempF>F", "-L", "64", "-H", "77", "-l", "blue", "-n", "green", "-h", "red"] 36000,
               Run Network "wlp3s0" ["-t", "<dev>: <rx>KB/<tx>KB", "-L", "0", "-H", "50", "-l", "green", "-n", "orange", "-h", "red"] 10,
               Run Memory ["-t", "Mem: <usedratio>%", "-L", "20", "-H", "80", "-l", "green", "-n", "orange", "-h", "red"] 10,
               Run Swap ["-L", "1", "-H", "5", "-l", "green", "-n", "orange", "-h", "red"] 10,
               Run Cpu ["-L", "5", "-H", "50", "-l", "green", "-n", "orange", "-h", "red"] 10,
               -- Run Com "uname" ["-s", "-r"] "" 0,
               Run Date "%m-%d-%Y %H:%M:%S" "date" 10,
               Run BatteryP ["BAT0"] ["-t", "Batt: <left>%", "-L", "15", "-H", "85", "-l", "red", "-n", "orange", "-h", "green", "-p", "3", "--", "-O", "<fc=green>On</fc>", "-o", "", "-L", "-15", "-H", "-5", "-l", "red", "-h", "green"] 600,
               Run ThermalZone 0 ["-t", "Temp: <temp>C", "-L", "40", "-H", "60", "-l", "orange", "-n", "green", "-h", "red"] 600,
               Run StdinReader ],
  sepChar = "%",
  alignSep = "}{",
  template = " %StdinReader% }{ | %cpu% %memory% %swap% %wlp3s0% | <fc=lightblue>%date%</fc> %KMDW% | %thermal0% %battery% "
  }
