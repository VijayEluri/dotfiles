Config {
  font = "xft:DejaVu Sans Mono:pixelsize:10",
  bgColor = "darkblue",
  fgColor = "lightblue",
  position = BottomW L 90,
  lowerOnStart = True,
  commands = [ Run Weather "KMDW" ["-t", "<tempF>F", "-L", "64", "-H", "77", "-l", "lightblue", "-n", "green", "-h", "red"] 36000,
               Run Network "eth0" ["-t", "<dev>: <rx>KB/<tx>KB", "-L", "0", "-H", "32", "-n", "green", "-h", "red"] 10,
               Run Memory ["-t", "Mem: <usedratio>%"] 10,
               Run Swap [] 10,
               Run Cpu ["-L", "3", "-H", "50", "-n", "green", "-h", "red"] 10,
               Run BatteryP ["BAT0"] ["-t", "Batt: <left>%", "-L", "20", "-H", "80", "-l", "red", "-h", "green", "-p", "3", "--", "-O", "<fc=green>On</fc>", "-o", "", "-L", "-15", "-H", "-5", "-l", "red", "-h", "green"] 600,
               Run ThermalZone 0 ["-t", "Temp: <temp>C", "-L", "40", "-H", "60", "-l", "green", "-h", "red"] 600,
               Run Com "uname" ["-s", "-r"] "" 0,
               Run Date "%m-%d-%Y %H:%M:%S" "date" 10,
               Run StdinReader ],
  sepChar = "%",
  alignSep = "}{",
  template = " %StdinReader% }{ | %cpu% %memory% %swap% %eth0% | <fc=#ee9a00>%date%</fc> %KMDW% | %thermal0% %battery% "
  }
